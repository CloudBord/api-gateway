{
  "GlobalConfiguration": {
    //"ServiceDiscoveryProvider": {
    //  "Namespace": "propnull",
    //  "Type": "KubernetesServiceDiscoveryProvider"
    //}
  },
  "Routes": [
    {
      //"ServiceName": "whiteboard-service",
      "UpstreamPathTemplate": "/api/boards",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7210
        }
      ],
      "DownstreamPathTemplate": "/boards",
      "DownstreamScheme": "http"
    },
    {
      //"ServiceName": "whiteboard-service",
      "UpstreamPathTemplate": "/api/boards/{id}",
      "UpstreamHttpMethod": [ "Delete" ],
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7210
        }
      ],
      "DownstreamPathTemplate": "/boards/{id}",
      "DownstreamScheme": "http"
    },
    {
      //"ServiceName": "whiteboard-service",
      "UpstreamPathTemplate": "/api/boards/{id}",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7210
        }
      ],
      "DownstreamPathTemplate": "/boards/{id}",
      "DownstreamScheme": "http",
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 10,
        "Limit": 4
      },
      "FileCacheOptions": {
        "TtlSeconds": 10
      }
    },
    {
      //"ServiceName": "whiteboard-service",
      "UpstreamPathTemplate": "/api/boards",
      "UpstreamHttpMethod": [ "Put" ],
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7210
        }
      ],
      "DownstreamPathTemplate": "/boards",
      "DownstreamScheme": "http"
    },
    {
      //"ServiceName": "session-handler",
      "UpstreamPathTemplate": "/party/{room}",
      "UpstreamHttpMethod": [ "GET", "DELETE", "PUT", "PATCH", "POST" ],
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5145
        }
      ],
      "DownstreamPathTemplate": "/{room}",
      "DownstreamScheme": "ws"
    },
    {
      //"ServiceName": "session-handler",
      "UpstreamPathTemplate": "/api/snapshot",
      "UpstreamHttpMethod": [ "POST" ],
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5145
        }
      ],
      "DownstreamPathTemplate": "/snapshot",
      "DownstreamScheme": "http"
    }
  ]
}